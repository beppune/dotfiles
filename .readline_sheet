
function rlsheet () {
	echo
	echo -e "\\e[38;2;255;0;255m.::| Some readline shortcuts |::.\\e[0m"
	echo
	local L1="\\e[38;2;255;0;255m Ctrl-a - Move to the start of the line\\e[0m"
	local L2="\\e[38;2;255;0;255m Ctrl-e - Move to the end of the line\\e[0m"
	local L3="\\e[38;2;255;0;255m Ctrl-f - Move forward a character\\e[0m"
	local L4="\\e[38;2;255;0;255m Ctrl-b - Move backward a character\\e[0m"
	local L5="\\e[38;2;255;0;255m Alt-f - Move forward to the end of the next word\\e[0m"
	local L6="\\e[38;2;255;0;255m Alt-b - Move backward to the start of the current or previous word\\e[0m"
	
	local L7="\\e[38;2;255;0;255m Ctrl-k - Cut forward to the end of the line\\e[0m"
	local L8="\\e[38;2;255;0;255m Ctrl-k - Cut backward to the end of the line\\e[0m"
	local L9="\\e[38;2;255;0;255m Alt-d - Cut forward to the end of the current word\\e[0m"
	local L0="\\e[38;2;255;0;255m Ctrl-w - Cut backward to the end of the current word\\e[0m"

	
	echo -e "$L1"
	echo -e "$L2"
	echo -e "$L3"
	echo -e "$L4"
	echo -e "$L5"
	echo -e "$L6"
	echo
	echo -e "$L7"
	echo -e "$L8"
	echo -e "$L9"
	echo -e "$L0"
	echo
}
